<?xml version='1.0' encoding='utf-8'?>
<widget id="able99.bnorth.demo" version="3.1.6" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>bnorth</name>
    <description>bnorth demo</description>
    <author email="" href="https://github.com/able99">able99 &lt;8846755@qq.com&gt;</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <icon src="../res/logo.png" />
    <plugin name="cordova-plugin-whitelist" spec="~1.3.3" />
    <engine name="android" spec="~7.1.4" />
</widget>
